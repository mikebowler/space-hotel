<table id="reservations">
	<tr>
		<th>Location</th>
		<th>Start date</th>
		<th>Nights</th>
		<th>Special instructions</th>
	</tr>
	<% @reservations.each do |row| %>
		<tr>
			<td><%= row[:location] %></td>
			<td><%= row[:start] %></td>
			<td style="text-align: right;"><%= row[:nights] %></td>
			<td><%= row[:instructions] %></td>
		</tr>
	<% end %>
</table>